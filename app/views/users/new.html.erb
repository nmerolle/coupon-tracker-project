<h1>Sign Up</h1>

<%= render partial: "layouts/errors", locals: {object: @user} %>

<%= form_with(model: @user) do |f| %>
<div>
<%= f.label :username %>
<%= f.text_field :username %>
</div>
<div>
<%= f.label :email %>
<%= f.email_field :email %>
</div>
<div>
<%= f.label :password %>
<%= f.password_field :password %>
</div>
<br><br>
<%= f.submit "Finish Signup" %>
<% end %>